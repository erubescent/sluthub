{
  "require": {
    "craftcms/cms": "3.0.18",
    "vlucas/phpdotenv": "^2.4.0",
    "ether/logs": "v3.0.0",
    "focuslabllc/craft-cheat-sheet": "v2.0.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
